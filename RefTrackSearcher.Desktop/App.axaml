<Application xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 x:Class="RefTrackSearcher.Desktop.App"
			 xmlns:local="using:RefTrackSearcher.Desktop"
			 RequestedThemeVariant="Default">

	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://RefTrackSearcher.Desktop/Styles/ThemeResources.axaml" />
			</ResourceDictionary.MergedDictionaries>
			<FontFamily x:Key="FontInter">avares://RefTrackSearcher.Desktop/Assets/Fonts/Inter-Regular.otf#Inter</FontFamily>
		</ResourceDictionary>
	</Application.Resources>

	<Application.Styles>
		<SimpleTheme />
		<StyleInclude Source="avares://RefTrackSearcher.Desktop/Styles/GlobalStyles.axaml" />

	</Application.Styles>

</Application>
